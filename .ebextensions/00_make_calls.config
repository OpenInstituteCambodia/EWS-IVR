container_commands:
  01_make_calls:
    command: "cat .ebextensions/make_calls_cron_job.txt > /etc/cron.d/make_calls_cron_job && chmod 644 /etc/cron.d/make_calls_cron_job"
  02_fix_permissions:
    command: "chmod -R 777 /var/app/current/storage/logs && ln -s /var/app/current/storage/logs /var/log/laravel"